#!/bin/bash

docker build -t $IMAGE_NAME --build-arg REACT_APP_ENVIRONMENT=$REACT_APP_ENVIRONMENT --build-arg REACT_APP_HTTP_API_URL=$REACT_APP_HTTP_API_URL --build-arg REACT_APP_TOKEN_KEY_NAME=$REACT_APP_TOKEN_KEY_NAME --build-arg REACT_APP_WIDGETS_CONFIG_NAME=$REACT_APP_WIDGETS_CONFIG_NAME --build-arg REACT_APP_WS_API_URL=$REACT_APP_WS_API_URL .
