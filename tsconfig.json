{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "jsx": "react-jsx",
    "module": "commonjs",
    "rootDir": "./",
    "baseUrl": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "resolveJsonModule": true
  },
  "include": [
    "./**/*"
  ]
}
